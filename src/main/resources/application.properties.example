# Configuração da API
# Copie este arquivo para application.properties e configure com seus valores
api.base.url=http://localhost:5000/api/

# Timeout de conexão (em segundos)
api.timeout=30

# EXEMPLOS DE CONFIGURAÇÃO:
# 
# Localhost:
# api.base.url=http://localhost:5000/api/
#
# Rede local:
# api.base.url=http://192.168.1.100:5000/api/
#
# Servidor remoto:
# api.base.url=https://api.seuservidor.com.br/api/
#
# IMPORTANTE: Sempre inclua /api/ no final da URL
